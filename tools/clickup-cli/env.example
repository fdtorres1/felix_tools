# ClickUp CLI Configuration
# Copy to ~/AGENTS.env and fill in your values

# Required: Personal API token from ClickUp
CLICKUP_API_TOKEN=pk_12345678_YOUR_API_TOKEN_HERE

# Optional: Default team (workspace) ID
CLICKUP_DEFAULT_TEAM_ID=

# Optional: Default list ID for create/update
CLICKUP_DEFAULT_LIST_ID=

# Optional: Telegram notifications for failures
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
